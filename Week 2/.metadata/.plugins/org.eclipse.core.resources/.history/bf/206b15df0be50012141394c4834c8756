
public class Vertice {
	private int id;
	private int colour;
	private int numberOfNeighbours;
	private int numberOfUncolouredNeighbours;
	private List<Integer> neighbourIDs;
	
	public Vertice(int idIn, int colourIn, int numberOfNeighboursIn, int numberOfUncolouredNeighboursIn){
		id = idIn;
		colour = colourIn;
		numberOfNeighbours = numberOfNeighboursIn;
		numberOfUncolouredNeighbours = numberOfUncolouredNeighboursIn;
	}
}
