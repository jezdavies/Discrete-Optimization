
public class Item {
	
	private int itemNumber;
	private float value;
	private float weight;
	
	public float getValue() {
		return value;
	}
	public void setValue(float value) {
		this.value = value;
	}
	public float getWeight() {
		return weight;
	}
	public void setWeight(float weight) {
		this.weight = weight;
	}
	
	public Item(int itemNumber_in,float value_in,float weight_in){
		itemNumber = itemNumber_in;
		setValue(value_in);
		setWeight(weight_in);
	}

}
